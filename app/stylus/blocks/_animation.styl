@keyframes swimRight
  0%
    left -200px
    transform scale(1)
    opacity 1

  33%
    transform scale(0.8)
    opacity .4

  66%
    transform scale(1.1)
    opacity .6

  100%
    left 110vw
    transform scale(1)
    opacity 1

@keyframes swimRightTop
  0%
    left -200px
    top 399px

  33%
    top 280px

  66%
    top 330px

  100%
    left 110vw
    top 399px

@keyframes swimRightTop2
  0%
    left -200px
    top 362px
    opacity 1

  50%
    top 460px

  100%
    left 110vw
    top 362px
    opacity 0

@keyframes swimLeft
  0%
    left auto
    right -200px

  100%
    right 100vw
    left auto

@keyframes swimLeft2
  0%
    left auto
    right -200px
    top 217px

  50%
    top 300px

  100%
    right 100vw
    left auto
    top 217px

@keyframes fadeInLeft
  from
    opacity 0
    transform translate3d(-100%, 0, 0)

  to
    opacity 1
    transform translate3d(0, 0, 0)
